{
    "name": "proto-to-typescript",
    "version": "0.0.1",
    "license": "apache",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "cosmos": "protoc --plugin=protoc-gen-as=./node_modules/.bin/as-proto-gen --proto_path=./src --as_out=../ ./src/cosmos.v1.proto && mv ../cosmos.v1.ts ../cosmos.ts && FILE_PATH=../cosmos.ts node index.js",
        "lint": "prettier -c ../*.ts",
        "format": "prettier --write -c ../*.ts"
    },
    "dependencies": {
        "as-proto-gen": "^0.2.3",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "fs": "^0.0.1-security",
        "proto-parser": "^0.0.7",
        "typescript": "^4.4.4",
        "yargs": "^17.2.1"
    },
    "devDependencies": {
        "prettier": "^2.4.1"
    }
}
